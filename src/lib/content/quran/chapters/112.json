{
    "id": 112,
    "name": "الإخلاص",
    "transliteration": "Al-Ikhlas",
    "translation": "The Sincerity",
    "type": "meccan",
    "total_verses": 4,
    "verses": {
        "1": "بِسْمِ ٱللَّهِ ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ قُلْ هُوَ ٱللَّهُ أَحَدٌ",
        "2": "ٱللَّهُ ٱلصَّمَدُ",
        "3": "لَمْ يَلِدْ وَلَمْ يُولَدْ",
        "4": "وَلَمْ يَكُن لَّهُۥ كُفُوًا أَحَدٌۢ"
    }
}