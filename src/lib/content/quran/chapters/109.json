{
    "id": 109,
    "name": "الكافرون",
    "transliteration": "Al-Kafirun",
    "translation": "The Disbelievers",
    "type": "meccan",
    "total_verses": 6,
    "verses": {
        "1": "بِسْمِ ٱللَّهِ ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ قُلْ يَـٰٓأَيُّهَا ٱلْكَـٰفِرُونَ",
        "2": "لَآ أَعْبُدُ مَا تَعْبُدُونَ",
        "3": "وَلَآ أَنتُمْ عَـٰبِدُونَ مَآ أَعْبُدُ",
        "4": "وَلَآ أَنَا۠ عَابِدٌ مَّا عَبَدتُّمْ",
        "5": "وَلَآ أَنتُمْ عَـٰبِدُونَ مَآ أَعْبُدُ",
        "6": "لَكُمْ دِينُكُمْ وَلِىَ دِينِ"
    }
}