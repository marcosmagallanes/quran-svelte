{
    "id": 108,
    "name": "الكوثر",
    "transliteration": "Al-Kawthar",
    "translation": "The Abundance",
    "type": "meccan",
    "total_verses": 3,
    "verses": {
        "1": "بِسْمِ ٱللَّهِ ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ إِنَّآ أَعْطَيْنَـٰكَ ٱلْكَوْثَرَ",
        "2": "فَصَلِّ لِرَبِّكَ وَٱنْحَرْ",
        "3": "إِنَّ شَانِئَكَ هُوَ ٱلْأَبْتَرُ"
    }
}