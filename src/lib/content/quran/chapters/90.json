{
    "id": 90,
    "name": "البلد",
    "transliteration": "Al-Balad",
    "translation": "The City",
    "type": "meccan",
    "total_verses": 20,
    "verses": {
        "1": "بِسْمِ ٱللَّهِ ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ لَآ أُقْسِمُ بِهَـٰذَا ٱلْبَلَدِ",
        "2": "وَأَنتَ حِلٌّۢ بِهَـٰذَا ٱلْبَلَدِ",
        "3": "وَوَالِدٍ وَمَا وَلَدَ",
        "4": "لَقَدْ خَلَقْنَا ٱلْإِنسَـٰنَ فِى كَبَدٍ",
        "5": "أَيَحْسَبُ أَن لَّن يَقْدِرَ عَلَيْهِ أَحَدٌ",
        "6": "يَقُولُ أَهْلَكْتُ مَالًا لُّبَدًا",
        "7": "أَيَحْسَبُ أَن لَّمْ يَرَهُۥٓ أَحَدٌ",
        "8": "أَلَمْ نَجْعَل لَّهُۥ عَيْنَيْنِ",
        "9": "وَلِسَانًا وَشَفَتَيْنِ",
        "10": "وَهَدَيْنَـٰهُ ٱلنَّجْدَيْنِ",
        "11": "فَلَا ٱقْتَحَمَ ٱلْعَقَبَةَ",
        "12": "وَمَآ أَدْرَىٰكَ مَا ٱلْعَقَبَةُ",
        "13": "فَكُّ رَقَبَةٍ",
        "14": "أَوْ إِطْعَـٰمٌ فِى يَوْمٍ ذِى مَسْغَبَةٍ",
        "15": "يَتِيمًا ذَا مَقْرَبَةٍ",
        "16": "أَوْ مِسْكِينًا ذَا مَتْرَبَةٍ",
        "17": "ثُمَّ كَانَ مِنَ ٱلَّذِينَ ءَامَنُوا۟ وَتَوَاصَوْا۟ بِٱلصَّبْرِ وَتَوَاصَوْا۟ بِٱلْمَرْحَمَةِ",
        "18": "أُو۟لَـٰٓئِكَ أَصْحَـٰبُ ٱلْمَيْمَنَةِ",
        "19": "وَٱلَّذِينَ كَفَرُوا۟ بِـَٔايَـٰتِنَا هُمْ أَصْحَـٰبُ ٱلْمَشْـَٔمَةِ",
        "20": "عَلَيْهِمْ نَارٌ مُّؤْصَدَةٌۢ"
    }
}