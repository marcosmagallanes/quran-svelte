{
    "id": 111,
    "name": "المسد",
    "transliteration": "Al-Masad",
    "translation": "The Palm Fiber",
    "type": "meccan",
    "total_verses": 5,
    "verses": {
        "1": "بِسْمِ ٱللَّهِ ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ تَبَّتْ يَدَآ أَبِى لَهَبٍ وَتَبَّ",
        "2": "مَآ أَغْنَىٰ عَنْهُ مَالُهُۥ وَمَا كَسَبَ",
        "3": "سَيَصْلَىٰ نَارًا ذَاتَ لَهَبٍ",
        "4": "وَٱمْرَأَتُهُۥ حَمَّالَةَ ٱلْحَطَبِ",
        "5": "فِى جِيدِهَا حَبْلٌ مِّن مَّسَدٍۭ"
    }
}