{
    "id": 94,
    "name": "الشرح",
    "transliteration": "Ash-Sharh",
    "translation": "The Relief",
    "type": "meccan",
    "total_verses": 8,
    "verses": {
        "1": "بِسْمِ ٱللَّهِ ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ أَلَمْ نَشْرَحْ لَكَ صَدْرَكَ",
        "2": "وَوَضَعْنَا عَنكَ وِزْرَكَ",
        "3": "ٱلَّذِىٓ أَنقَضَ ظَهْرَكَ",
        "4": "وَرَفَعْنَا لَكَ ذِكْرَكَ",
        "5": "فَإِنَّ مَعَ ٱلْعُسْرِ يُسْرًا",
        "6": "إِنَّ مَعَ ٱلْعُسْرِ يُسْرًا",
        "7": "فَإِذَا فَرَغْتَ فَٱنصَبْ",
        "8": "وَإِلَىٰ رَبِّكَ فَٱرْغَب"
    }
}