{
    "id": 105,
    "name": "الفيل",
    "transliteration": "Al-Fil",
    "translation": "The Elephant",
    "type": "meccan",
    "total_verses": 5,
    "verses": {
        "1": "بِسْمِ ٱللَّهِ ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِأَصْحَـٰبِ ٱلْفِيلِ",
        "2": "أَلَمْ يَجْعَلْ كَيْدَهُمْ فِى تَضْلِيلٍ",
        "3": "وَأَرْسَلَ عَلَيْهِمْ طَيْرًا أَبَابِيلَ",
        "4": "تَرْمِيهِم بِحِجَارَةٍ مِّن سِجِّيلٍ",
        "5": "فَجَعَلَهُمْ كَعَصْفٍ مَّأْكُولٍۭ"
    }
}