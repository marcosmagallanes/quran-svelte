{
    "id": 114,
    "name": "الناس",
    "transliteration": "An-Nas",
    "translation": "Mankind",
    "type": "meccan",
    "total_verses": 6,
    "verses": {
        "1": "بِسْمِ ٱللَّهِ ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ قُلْ أَعُوذُ بِرَبِّ ٱلنَّاسِ",
        "2": "مَلِكِ ٱلنَّاسِ",
        "3": "إِلَـٰهِ ٱلنَّاسِ",
        "4": "مِن شَرِّ ٱلْوَسْوَاسِ ٱلْخَنَّاسِ",
        "5": "ٱلَّذِى يُوَسْوِسُ فِى صُدُورِ ٱلنَّاسِ",
        "6": "مِنَ ٱلْجِنَّةِ وَٱلنَّاسِ"
    }
}