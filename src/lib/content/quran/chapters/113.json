{
    "id": 113,
    "name": "الفلق",
    "transliteration": "Al-Falaq",
    "translation": "The Daybreak",
    "type": "meccan",
    "total_verses": 5,
    "verses": {
        "1": "بِسْمِ ٱللَّهِ ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ قُلْ أَعُوذُ بِرَبِّ ٱلْفَلَقِ",
        "2": "مِن شَرِّ مَا خَلَقَ",
        "3": "وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ",
        "4": "وَمِن شَرِّ ٱلنَّفَّـٰثَـٰتِ فِى ٱلْعُقَدِ",
        "5": "وَمِن شَرِّ حَاسِدٍ إِذَا حَسَدَ"
    }
}